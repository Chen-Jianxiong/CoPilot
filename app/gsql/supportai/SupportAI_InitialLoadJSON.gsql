CREATE LOADING JOB load_documents_content_json {
    DEFINE FILENAME DocumentContent;

    LOAD DocumentContent TO VERTEX Content VALUES($"doc_id", $"content", gsql_current_time_epoch()) USING JSON_FILE="true";
    LOAD DocumentContent TO VERTEX Document VALUES($"doc_id", _, gsql_current_time_epoch(), _, _) USING JSON_FILE="true";
}